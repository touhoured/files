          "address": "tls://223.5.5.5",
          "detour": "direct"
        }
      ],
      "pre_dns_rules": [
        {
          "type": "logical",
          "mode": "or",
          "rules": [
            {
              "domain_suffix": [
                "cdn77.org",
                "cdn77.com"
              ]
            },
            {
              "process_name": [
                "gephgui-wry.exe",
                "geph5-client-gui.exe",
                "geph4-client.exe",
                "geph5-client.exe",
                "geph5-client",
