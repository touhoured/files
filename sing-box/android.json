{
  "log": {
    "disabled": false,
    "level": "debug",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "yacd",
      "store_selected": false
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "proxy",
        "address": "tcp://cloudflare-dns.com",
        "address_resolver": "proxy-ip",
        // "strategy": "prefer_ipv4",
        "detour": "proxy"
      },
      {
        "tag": "proxy-ip",
        "address": "tcp://1.1.1.1",
        "detour": "proxy"
      },
      {
        "tag": "bypass",
        "address": "tls://dns.pub",
        "address_resolver": "bypass-ip",
        // "strategy": "prefer_ipv6",
        "detour": "direct"
      },
      {
        "tag": "bypass-ip",
        "address": "tls://223.5.5.5",
        "detour": "direct"
      },
      {
        "tag": "bypass-google",
        "address": "tls://dns.google",
        "address_resolver": "direct",
        "detour": "direct"
      },
      {
        "tag": "direct",
        "address": "tls://223.5.5.5",
        "detour": "direct"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "clash_mode": "global",
        "server": "proxy"
      },
      {
        "clash_mode": "direct",
        "server": "direct"
      },
      {
        "geosite": "private",
        "server": "direct"
      },
      {
        "geosite": "category-ads-all",
        "disable_cache": true,
        "server": "block"
      },
      {
        "domain_suffix": [
          "winos.me",
          "kinh.cc",
          "dandanplay.net",
          "metacubex.one",
          "dl.google.com",
          "yacd.haishan.me"
        ],
        "server": "bypass-google"
      },
      {
        "domain_suffix": [
          "geilijiasu.com"
        ],
        "server": "bypass"
      },
      {
        "process_name": "GenshinImpact.exe",
        "domain_suffix": "dispatchosglobal.yuanshen.com",
        "server": "proxy"
      },
      {
        "geosite": "category-games@cn",
        "server": "bypass"
      },
      {
        "geosite": "geolocation-!cn",
        "server": "proxy"
      },
      {
        "geosite": "cn",
        "server": "bypass"
      }
    ]
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "inet4_address": "172.19.0.1/30",
      "inet6_address": "fdfe:dcba:9876::1/126",
      "auto_route": true,
      "strict_route": true,
      "sniff": true,
      // "sniff_override_destination": true,
      "type": "tun"
    },
    {
      "tag": "mixed-in",
      "listen": "0.0.0.0",
      "listen_port": 7890,
      "set_system_proxy": false,
      "sniff": true,
      // "sniff_override_destination": true,
      "type": "mixed"
    }
  ],
