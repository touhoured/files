      //   "server": "dns-refused"
      // }
    ],
    "servers": [
      {
        "tag": "dns-proxy",
        "address": "tcp://8.8.8.8",
        // "address": "tcp://dns.google",
        // "address_resolver": "dns-proxy-resolver",
        "detour": "out-proxy"
