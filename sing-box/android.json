  "dns": {
    // "final": "dns-proxy",
    // "strategy": "ipv4_only",
    "reverse_mapping": true,
    "independent_cache": true,
    "fakeip": {
      "enabled": true,
      "inet6_range": "fc00::/18",
      "inet4_range": "198.18.0.0/15"
    },
    "rules": [
      {
        "domain_suffix": [
          "cdn77.com",
          "cdn77.org"
        ],
        "server": "dns-local"
      },
      {
        "process_name": [
          "geph5-client-gui.exe",
          "geph4-client.exe",
          "geph5-client.exe",
          "geph5-client",
          "geph4-client"
        ],
        "server": "dns-local"
      },
      // { // if outbound use domain
      //   "disable_cache": true,
      //   "outbound": "any",
      //   // "action": "route-options",
      //   "server": "dns-local"
      // },
      {
        "clash_mode": "direct",
        "server": "dns-local"
      },
      {
        "clash_mode": "global",
        "server": "dns-proxy"
      },
      {
        "rule_set": [
          "geosite-anti-ip-attribution-reject",
          "geosite-ADBlock-MeepoMaster",
          "geosite-ADBlock-Cats-Team",
          // "geosite-ADBlock-ppfeufer",
          // "geosite-ADBlock-AdGuardSDNSFilter",
          "geosite-category-httpdns-cn",
          "geosite-category-ads-all",
          "custom-block"
        ],
        // "method": "default",
        // "no_drop": false,
        "action": "reject"
      },
      {
        "rule_set": [
          "custom-direct"
        ],
        "server": "out-direct"
      },
      {
        "domain_suffix": [
          "extshort.weixin.qq.com",
          "short.weixin.qq.com"
        ],
        "server": "out-wechat"
      },
      {
        "rule_set": "geosite-googlefcm",
        "server": "dns-googlefcm"
      },
      {
        "rule_set": "geosite-onedrive",
        "server": "dns-onedrive"
      },
      {
        "rule_set": [
          "geosite-geolocation-cn",
          "geosite-tld-cn",
          "geosite-cn"
        ],
        "server": "dns-direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "client_subnet": "114.114.114.114/24", // Any China client IP address
        // "action": "route-options",
        "server": "dns-proxy"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "query_type": [
          "AAAA",
          "A"
        ],
        // "rewrite_ttl": 1,
        // "action": "route-options",
        "server": "dns-fakeip"
      },
      // {
      //   "rule_set": "geosite-geolocation-!cn",
      //   "query_type": [
      //     "CNAME"
      //   ],
      //   "server": "dns-proxy"
      // },
      {
        "rule_set": "geosite-private",
        "server": "dns-local"
      },
      // {
      //   "query_type": [
