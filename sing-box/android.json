      // {
      //   "tag": "dns-proxy-resolver",
      //   "address": "tcp://8.8.8.8",
      //   "detour": "out-proxy"
      // },
      {
        "tag": "dns-wechat",
        "address": "tcp://223.5.5.5",
        "detour": "out-wechat"
      },
      {
        "tag": "dns-googlefcm",
        "address": "tcp://223.5.5.5",
        "detour": "out-googlefcm"
      },
      {
        "tag": "dns-onedrive",
        "address": "tcp://223.5.5.5",
        "detour": "out-onedrive"
      },
      {
        "tag": "dns-direct",
        // "address": "tls://dns.pub",
        "address": "tls://dns.alidns.com",
        "address_resolver": "dns-local",
        "detour": "out-direct"
      },
      {
        "tag": "dns-local",
        // "address": "dhcp://auto",
        "address": "local",
        // "address": "223.5.5.5",
        "detour": "out-direct"
      },
      {
        "tag": "dns-fakeip",
        "address": "fakeip"
      },
      // {
      //   "tag": "dns-success",
      //   "address": "rcode://success"
      // },
      // {
      //   "tag": "dns-block",
      //   "address": "rcode://refused"
      // }
    ]
