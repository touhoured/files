    },
    {
      "tag": "out-2090",
      "server": "127.0.0.1",
      "server_port": 2080,
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-direct",
      "type": "direct"
    }
  ],
  "inbounds": [
    {
      "tag": "in-tun",
      "address": [
        "fd00::1/126",
        "172.19.0.1/30"
      ],
      "auto_route": true,
      "strict_route": true,
      "platform": {
        "http_proxy": {
          "server": "127.0.0.1",
          "server_port": 7890,
          // "bypass_domain": [ "dl.google.com" ],
          // "match_domain": [ "github.com" ],
          "enabled": true
        }
      },
      // "stack": "mixed",
      "type": "tun"
    },
    {
      "tag": "in-dns",
      "listen": "::",
      "listen_port": 1053,
      "type": "direct"
    },
    {
      "tag": "in-mixed",
      "listen": "::",
      "listen_port": 7890,
      // "set_system_proxy": true,
      "type": "mixed"
    }
  ],
  "log": {
