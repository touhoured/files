  "outbounds": [
    {
      "tag": "out-proxy",
      "outbounds": [
        // "out-fallback",
        "out-9999",
        "out-9909",
        "out-2080",
        "out-2090",
        "out-direct"
      ],
      "type": "selector"
    },
    {
      "tag": "out-final",
      "outbounds": [
        // "out-fallback",
        "out-proxy",
        "out-direct"
