// first launch need download rules-set.
// or change dns.final to "out-direct"
{
  "route": {
    "final": "out-proxy",
    "auto_detect_interface": true,
    // "default_network_strategy": "fallback",
    // "default_fallback_delay": "300ms",
    // "default_interface": "WLAN",
    "rules": [
      {
        // "inbound": "in-tun",
        // "timeout": "1s",
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "process_name": [
          "geph5-client-gui.exe",
          "geph4-client.exe",
          "geph5-client.exe",
          "geph5-client",
          "geph4-client"
        ],
        "outbound": "out-direct"
      },
      {
        "clash_mode": "direct",
        "outbound": "out-direct"
      },
      {
        "clash_mode": "global",
        "outbound": "out-proxy"
      },
      { // dot quic stun
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "protocol": "stun"
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "quic"
          }
        ],
        // "method": "default",
        // "no_drop": false,
        "action": "reject"
      },
      {
        "rule_set": [
          "geosite-anti-ip-attribution-reject",
          "geosite-ADBlock-MeepoMaster",
          "geosite-ADBlock-Cats-Team",
          // "geosite-ADBlock-ppfeufer",
          // "geosite-ADBlock-AdGuardSDNSFilter",
          "geosite-category-httpdns-cn",
          "geosite-category-ads-all",
          "custom-block"
        ],
        // "method": "default",
        // "no_drop": false,
        "action": "reject"
      },
      {
        "rule_set": [
          "custom-direct"
        ],
        "outbound": "out-direct"
      },
      // {
      //   "domain_suffix": [
      //     "biliapi.net",
      //     "biliapi.com",
      //     "bilibili.com"
      //   ],
      //   "ip_cidr": "203.107.1.0/24",
      //   "outbound": "out-bilibili"
      // },
      {
        "rule_set": "geosite-anti-ip-attribution-direct",
        "outbound": "out-direct"
      },
      {
        "rule_set": "geosite-anti-ip-attribution-proxy",
        "outbound": "out-anti-ip-attribution"
      },
      {
        "domain_suffix": [
          "extshort.weixin.qq.com",
          "short.weixin.qq.com"
        ],
        "outbound": "out-wechat"
      },
      {
        "rule_set": "geosite-googlefcm",
        "outbound": "out-googlefcm"
      },
      {
        "rule_set": "geosite-onedrive",
        "outbound": "out-onedrive"
      },
      {
        "rule_set": [
          "geosite-geolocation-cn",
          "geosite-tld-cn",
          "geosite-cn",
          "geoip-cn"
        ],
        "outbound": "out-direct"
      },
      {
        "ip_is_private": true,
        "rule_set": "geosite-private",
        "outbound": "out-direct"
      },
      {
        "outbound": "out-final"
      }
    ],
    "rule_set": [
      {
        "tag": "custom-block",
        "rules": [
          {
            "domain": [
              "block.test", // only test
              "black.qimo.ink", // biliroming blacklist
              "2fa.qwq2333.top", // QAuxiliary
              "api.qwq2333.top", // QAuxiliary
              "ntp.msn.cn" // edge startpage
            ],
            "domain_suffix": [
              ".mcdn.bilivideo.cn",
              ".szbdyd.com"
            ],
            "domain_regex": [
              "cn-.+\\.bilivideo\\.com$",
              "-pcdn-.?\\.biliapi\\.net$"
            ]
          }
        ],
        "type": "inline"
      },
      {
        "tag": "custom-direct",
        "rules": [
          {
            "domain_suffix": [
              "ghgo.xyz",
              "ghproxy.cc",
              "deeplx.org",
              "dandanplay.net",
              "metacubex.one",
              "yacd.haishan.me"
            ]
          }
        ],
        "type": "inline"
      },
      {
        "tag": "geosite-anti-ip-attribution-reject",
        "format": "binary",
        "url": "https://ghgo.xyz/https://raw.githubusercontent.com/touhoured/sing-box-geosite/main/rule/rule-provider-reject.srs",
        "download_detour": "out-direct",
        // "update_interval": "1d",
        "type": "remote"
      },
      {
        "tag": "geosite-anti-ip-attribution-direct",
        "format": "binary",
        "url": "https://ghgo.xyz/https://raw.githubusercontent.com/touhoured/sing-box-geosite/main/rule/rule-provider-direct.srs",
