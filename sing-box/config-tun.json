  },
  "dns": {
    "rules": [
      {
        "process_name": [
          "geph5-client-gui.exe",
          "geph4-client.exe",
          "geph5-client.exe",
          "geph5-client",
          "geph4-client"
        ],
        "action": "route",
        "server": "dns-geph"
      },
      {
        "clash_mode": "Global",
        "action": "route",
        "server": "dns-proxy"
      },
      {
        "clash_mode": "Direct",
        "action": "route",
        "server": "dns-local"
      },
      // { // if outbound use domain
      //   "outbound": "any",
      //   "server": "dns_resolver"
      // },
      { // bypass lan, new priority, may affect match
        "rule_set": "geosite-private",
        "action": "route",
        "server": "dns-local"
      },
      {
        "rule_set": [
          "geosite-AdGuardSDNSFilter",
          "geosite-category-httpdns-cn",
          "geosite-category-ads-all"
        ],
        "domain": [
          "black.qimo.ink",
          "sgameicon.ecology.smoba.qq.com",
          "httpdns.bilivideo.com",
          "ntp.msn.cn"
        ],
        "domain_suffix": [
          ".qwq2333.top",
          ".mcdn.bilivideo.cn",
          ".szbdyd.com"
        ],
        "domain_regex": [
          "cn-.+\\.bilivideo\\.com$",
          "-pcdn-.?\\.biliapi\\.net$"
        ],
        "disable_cache": true,
        "action": "route-options",
        "server": "dns-success"
      },
      {
        "rule_set": [
          "geosite-geolocation-cn",
          "geosite-tld-cn",
          "geosite-cn"
        ],
        "action": "route",
        "server": "dns-direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "client_subnet": "1.0.1.0/24", // Any China client IP address
        "action": "route-options",
        "server": "dns-proxy"
      },
      {
        "query_type": [
          "CNAME",
          "AAAA",
          "A"
        ],
        "invert": true,
        "disable_cache": true,
        "action": "route-options",
        "server": "dns-refused"
      }
    ],
    "final": "dns-proxy",
    // "strategy": "ipv4_only",
    "servers": [
      // h3://1.1.1.1/dns-query
      // https://1.1.1.1/dns-query
      // tcp://1.1.1.1
      // 127.0.0.1:15353
      // quic://dns.adguard-dns.com
      // 9.9.9.11
      {
        "tag": "dns-proxy",
        "address": "https://8.8.8.8/dns-query", // 1.1.1.1 unsupport client subnet
        // "strategy": "ipv4_only",
        // "address_resolver": "dns-local",
        "detour": "out-proxy"
      },
      {
        "tag": "dns-direct",
        "address": "https://223.5.5.5/dns-query",
        // "address_resolver": "dns-local",
        "detour": "out-direct"
      },
      {
        "tag": "dns-geph",
        "address": "https://1.0.0.1/dns-query",
        // "address_resolver": "dns-local",
        "detour": "out-direct"
      },
      {
        "tag": "dns-local",
        // "address": "dhcp://auto",
        // "address": "local",
        "address": "223.5.5.5",
        "detour": "out-direct"
      },
      {
        "tag": "dns-success",
        "address": "rcode://success"
      },
      {
        "tag": "dns-block",
        "address": "rcode://refused"
      }
    ]
  },
  "outbounds": [
    {
      "tag": "out-proxy",
      "outbounds": [
        "out-geph5",
        "out-fallback",
        "out-geph4",
        "out-geph4-1",
        "out-geph4-2",
        "out-geph4-3",
        "out-geph4-4"
      ],
      "type": "selector"
    },
    {
      "tag": "out-fallback",
      "outbounds": [
        "out-geph5",
        "out-geph4",
        "out-geph4-1",
        "out-geph4-2",
        "out-geph4-3",
        "out-geph4-4"
      ],
      "url": "http://www.apple.com/library/test/success.html",
      "interval": "30s",
      "tolerance": 9999,
      "type": "urltest"
    },
    {
      "type": "selector",
      "tag": "out-bilibili",
      "outbounds": [
        "out-proxy",
        "out-direct"
      ]
    },
    {
      "type": "selector",
      "tag": "out-bilibili-cdn-cn",
      "outbounds": [
        "out-direct",
        "out-proxy"
      ]
    },
    {
      "type": "selector",
      "tag": "out-bilibili-cdn-global",
      "outbounds": [
        "out-direct",
        "out-proxy"
      ]
    },
    {
      "type": "selector",
      "tag": "out-weixin",
      "outbounds": [
        "out-proxy",
        "out-direct"
      ]
    },
    {
      "type": "selector",
      "tag": "out-googlefcm",
      "outbounds": [
        "out-direct",
        "out-proxy"
      ]
    },
    {
      "type": "selector",
      "tag": "out-onedrive",
      "outbounds": [
        "out-direct",
        "out-proxy"
      ]
    },
    {
      "tag": "out-geph5",
      "server": "127.0.0.1",
      "server_port": 9999,
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-geph4",
      "server": "127.0.0.1",
      "server_port": 9909,
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-geph4-1",
      "server": "127.0.0.1",
      "server_port": 9908,
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-geph4-2",
      "server": "127.0.0.1",
      "server_port": 9907,
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-geph4-3",
      "server": "127.0.0.1",
      "server_port": 9906,
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-geph4-4",
      "server": "127.0.0.1",
      "server_port": 9905,
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-direct",
      "type": "direct"
    }
  ],
  "inbounds": [
    {
      "tag": "in-tun",
      "address": [
        "fd00::1/126",
        "172.19.0.1/30"
      ],
      "auto_route": true,
      "strict_route": true,
      "stack": "mixed",
      "type": "tun"
    },
    {
      "tag": "in-dns",
      "listen": "::",
      "listen_port": 1053,
      "type": "direct"
    },
    {
      "tag": "in-mixed",
      "listen": "::",
      "listen_port": 7890,
      // "set_system_proxy": true,
      "type": "mixed"
    }
  ],
  "log": {
    "disabled": false,
    "level": "trace",
    "timestamp": false
  },
  "experimental": {
    "cache_file": {
      "store_rdrc": true,
      "rdrc_timeout": "1d",
      "path": "cache.db",
      "enabled": true
