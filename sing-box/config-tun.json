{
  "route": {
    "final": "out-proxy", // default proxy, whilelist mode
    "auto_detect_interface": true, // disable to fix multi interfaces
    // "default_interface": "WLAN", // disable to fix multi interfaces
    "rules": [
      { // bypass core, but look like cannot bypass dns request
        "process_name": [
          "geph5-client-gui.exe",
          "geph4-client.exe",
          "geph5-client.exe",
          "geph5-client",
          "geph4-client"
        ],
        "action": "route",
        "outbound": "out-direct"
      },
      {
        "clash_mode": "Direct",
        "action": "route",
        "outbound": "out-direct"
      },
      {
        "inbound": "tun-in",
        // "timeout": "1s",
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "clash_mode": "Global",
        "action": "route",
        "outbound": "out-proxy"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "stun"
          },
          {
            "protocol": "quic"
          }
        ],
        "method": "default",
        "no_drop": false,
        "action": "reject"
      },
      { // bypass lan, new priority, may affect match
      "ip_is_private": true,
      "rule_set": "geosite-private",
      "action": "route",
        "outbound": "out-direct"
      },
      { // ad
        "ip_cidr": [ // bilibili
          "47.101.175.206/32",
          "47.100.123.169/32",
          "120.46.169.234/32",
          "121.36.72.124/32",
          "116.63.10.135/32",
          "122.9.7.134/32",
          "117.185.228.108/32",
          "117.144.238.29/32",
          "122.9.13.79/32",
          "122.9.15.129/32",
          "101.91.140.224/32",
          "101.91.140.124/32",
          "114.116.215.110/32",
          "116.63.10.31/32",
          "112.64.218.119/32",
          "112.65.200.117/32"
        ],
        "action": "reject"
      },
      {
        "domain_suffix": [
          "proxy.ghproxy.com",
          "ghp.ci",
          "ghproxy.cc",
          "xaidc.com",
          "cjjd15.com",
          "cjjd18.com",
          "cjjd20.com",
          "7moor-fs1.com",
          "kanxue.com",
          "deeplx.org",
          "wuyou.net",
          "ghxi.com",
          "ly93.cc",
          "loli.io",
          "dl.sourceforge.net",
          "asmr-100.com",
          "dockerproxy.com",
          "winos.me",
          "kinh.cc",
          "dandanplay.net",
          "geilijiasu.com",
          "metacubex.one",
          "dl.google.com",
          "yacd.haishan.me"
        ],
        "action": "route",
        "outbound": "out-direct"
      },
      {
        "domain_suffix": [
          "bilivideo.com",
          "bilivideo.cn",
          "hdslb.net",
          "hdslb.com"
        ],
        "action": "route",
        "outbound": "out-bilibili-cdn-cn"
      },
      {
        "domain": "upos-hz-mirrorakam.akamaized.net",
        "action": "route",
        "outbound": "out-bilibili-cdn-global"
      },
      {
        "domain_suffix": [
          "biliapi.net",
          "biliapi.com",
          "bilibili.com"
        ],
        "ip_cidr": "203.107.1.0/24",
        "action": "route",
        "outbound": "out-bilibili"
      },
      {
        "domain_suffix": "short.weixin.qq.com",
        "action": "route",
        "outbound": "out-weixin"
      },
      {
        "rule_set": "geosite-googlefcm",
        "action": "route",
        "outbound": "out-googlefcm"
      },
      {
        "rule_set": "geosite-onedrive",
        "action": "route",
        "outbound": "out-onedrive"
      },
      {
        "rule_set": [
          "geosite-geolocation-cn",
          "geosite-tld-cn",
          "geosite-cn",
          "geoip-cn"
        ],
        "action": "route",
        "outbound": "out-direct"
      }
    ],
    "rule_set": [
      {
        "tag": "geosite-private",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-private.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-category-httpdns-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-httpdns-cn.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-ads-all.srs",
