    "clash_api": {
      "default_mode": "Rule",
      "access_control_allow_private_network": true,
      "secret": "touhoured",
      "external_ui": "xd",
      "external_ui_download_url": "https://ghp.ci/https://github.com/MetaCubeX/metacubexd/archive/gh-pages.zip",
