  "dns": {
    "rules": [
      {
        "process_name": [
          "geph5-client-gui.exe",
          "geph4-client.exe",
          "geph5-client.exe",
          "geph5-client",
          "geph4-client"
        ],
        "server": "dns-geph"
      },
      {
        "clash_mode": "Global",
        "server": "dns-proxy"
      },
      {
        "clash_mode": "Direct",
        "server": "dns-local"
      },
      { // bypass lan, new priority, may affect match
        "rule_set": "geosite-private",
        "server": "dns-local"
      },
      {
        "rule_set": [
          "geosite-category-httpdns-cn",
          "geosite-category-ads-all"
        ],
        "domain": [
          "httpdns.bilivideo.com",
          "ntp.msn.cn"
        ],
        "domain_suffix": [
          ".mcdn.bilivideo.cn",
          ".szbdyd.com"
        ],
        "domain_regex": [
          "cn-.+\\.bilivideo\\.com$",
          "-pcdn-.?\\.biliapi\\.net$"
        ],
        "disable_cache": true,
        "server": "dns-success"
      },
      {
        "rule_set": [
          // "geosite-apple@cn",
          // "geosite-microsoft@cn",
          // "geosite-category-games@cn",
          "geosite-geolocation-cn",
          "geosite-tld-cn",
          "geosite-cn"
        ],
        "server": "dns-direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "client_subnet": "1.0.1.0/24", // Any China client IP address
        "server": "dns-proxy"
      }
    ],
    // "final": "dns-proxy",
    // "strategy": "ipv4_only"
    "servers": [
      // h3://1.1.1.1/dns-query
      // https://1.1.1.1/dns-query
      // tcp://1.1.1.1
      // 127.0.0.1:15353
      // quic://dns.adguard-dns.com
      // 9.9.9.11
      {
        "tag": "dns-proxy",
        "address": "https://8.8.8.8/dns-query", // 1.1.1.1 unsupport client subnet
        // "strategy": "ipv4_only",
        // "address_resolver": "dns-subnet",
        "detour": "out-proxy"
      },
      {
        "tag": "dns-direct",
        "address": "https://223.5.5.5/dns-query",
        "detour": "out-direct"
      },
      {
        "tag": "dns-geph",
        "address": "https://1.0.0.1/dns-query",
        "detour": "out-direct"
      },
      {
        "tag": "dns-local",
        "address": "local",
        "detour": "out-direct"
      },
      {
        "tag": "dns-success",
        "address": "rcode://success"
      },
      {
        "tag": "dns-block",
        "address": "rcode://refused"
      }
    ]
  },
  "outbounds": [
