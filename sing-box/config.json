  "dns": {
    "final": "proxy",
    "strategy": "prefer_ipv4",
    "reverse_mapping": true,
    "independent_cache": true,
    "rules": [
      {
        "rule_set": "rule-geph",
        "server": "local"
      },
      {
        "domain_suffix": "ts.net",
        "server": "tailscale"
      },
      {
        "rule_set": "geosite-private",
        "server": "local"
      },
      {
        "clash_mode": "global",
        "server": "proxy"
      },
      {
        "clash_mode": "Direct",
        "server": "local"
      },
      {
        "rule_set": [
          "rule-anti-ip-attribution-reject-noip",
          "rule-MeepoMaster-adg-dns",
          "rule-Cats-Team-adrules-singbox",
          "geosite-category-httpdns-cn",
          "geosite-category-ads-all",
          "rule-custom-block"
        ],
        "action": "reject"
      },
      {
        "rule_set": "rule-custom-direct",
        "server": "local"
      },
      {
        "rule_set": "rule-anti-ip-attribution-direct-noip",
        "server": "local"
      },
      {
        "rule_set": "rule-anti-ip-attribution-proxy-noip",
        "server": "anti-ip-attribution"
      },
      {
        "rule_set": "rule-wechat",
        "server": "wechat"
      },
      {
        "rule_set": "geosite-googlefcm",
        "server": "googlefcm"
      },
      {
        "rule_set": "geosite-onedrive",
        "server": "onedrive"
      },
      {
        "rule_set": [
          // "geosite-geolocation-cn",
          // "geosite-tld-cn",
          "geosite-cn"
        ],
        "server": "local"
      },
      {
        "clash_mode": "Rule",
        "server": "default"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        // "client_subnet": "114.114.114.114/24",
        "server": "local"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip"
      }
    ],
    "servers": [
      {
        "tag": "proxy",
        "detour": "proxy",
        "server": "8.8.8.8",
        "type": "https"
      },
      {
        "tag": "anti-ip-attribution",
        "detour": "anti-ip-attribution",
        "server": "223.5.5.5",
