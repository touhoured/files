{
  "route": {
    "final": "proxy",
    "default_domain_resolver": "local",
    // "auto_detect_interface": true,
    "rules": [
      {
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "rule_set": "rule-geph",
        "outbound": "direct"
      },
      {
        "ip_cidr": [
          "fd7a:115c:a1e0::/48",
          "100.64.0.0/10"
        ],
        "outbound": "tailscale"
      },
      {
        "domain_suffix": "ts.net",
        "outbound": "tailscale"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-private",
        "outbound": "direct"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "proxy"
      },
      {
        "rule_set": "rule-custom-direct",
        "outbound": "direct"
      },
      {
        "rule_set": "rule-anti-ip-attribution-direct",
        "outbound": "direct"
      },
      {
        "rule_set": "rule-anti-ip-attribution-proxy",
        "outbound": "anti-ip-attribution"
      },
      {
        "rule_set": "rule-wechat",
        "outbound": "wechat"
      },
      {
        "rule_set": "geosite-googlefcm",
        "outbound": "googlefcm"
      },
      {
        "rule_set": "geosite-onedrive",
        "outbound": "onedrive"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "outbound": "direct"
      },
      // {
      //   "rule_set": "geosite-geolocation-!cn",
      //   "outbound": "proxy"
      // },
      {
        "clash_mode": "Rule",
        "outbound": "default"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geoip-cn"
          },
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          }
        ],
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "tag": "rule-wechat",
        "rules": [
          {
            "domain_suffix": [
              "extshort.weixin.qq.com",
              "short.weixin.qq.com"
            ]
          }
        ],
        "type": "inline"
      },
      {
        "tag": "rule-custom-block",
        "rules": [
          {
            "domain": [
              "block.test", // only test
              "black.qimo.ink", // biliroming blacklist
              "2fa.qwq2333.top", // QAuxiliary
              "api.qwq2333.top", // QAuxiliary
              "ntp.msn.cn" // edge startpage
            ],
            "domain_suffix": [
              ".mcdn.bilivideo.cn",
              ".szbdyd.com"
            ],
            "domain_regex": [
              "cn-.+\\.bilivideo\\.com$",
              "-pcdn-.?\\.biliapi\\.net$"
            ]
          }
        ],
        "type": "inline"
      },
      {
        "tag": "rule-custom-direct",
        "rules": [
          {
            "domain_suffix": [
              "bsbsb.top",
              "ghfast.top",
              "ghproxy.cc",
              "deeplx.org",
              "dandanplay.net",
              "metacubex.one",
              "yacd.haishan.me"
            ]
          }
        ],
        "type": "inline"
      },
      {
        "tag": "rule-geph",
        "rules": [
          {
            "domain_suffix": [
              "cdn77.org",
              "cdn77.com"
            ]
          },
          {
            "process_name": [
              "gephgui-wry.exe",
              "geph5-client-gui.exe",
              "geph4-client.exe",
              "geph5-client.exe",
              "geph5-client",
              "geph4-client"
            ]
          }
        ],
        "type": "inline"
      },
      {
        "tag": "rule-anti-ip-attribution-reject-noip",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/touhoured/sing-box-geosite/main/rule/rule-provider-reject-noip.srs",
        "download_detour": "direct",
        "type": "remote"
      },
      {
        "tag": "rule-anti-ip-attribution-direct-noip",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/touhoured/sing-box-geosite/main/rule/rule-provider-direct-noip.srs",
