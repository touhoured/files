{
  "route": {
    "final": "proxy",
    "auto_detect_interface": true,
    // "default_interface": "WLAN",
    "rules": [
      {
        "process_name": [
          "geph4-client.exe"
        ],
        "outbound": "direct"
      },
      {
        "clash_mode": "global",
        "outbound": "proxy"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"
      },
      {
        "protocol": "quic",
        "outbound": "block"
      },
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      { 
        "process_name": [
            "aria2.exe",
            "qbittorrent.exe",
            "IDMan.exe"
        ],
        "outbound": "bypass"
      },
      {
        "geosite": "category-ads-all",
        "outbound": "block"
      },
      {
        "process_name": "GenshinImpact.exe",
        "outbound": "proxy"
      },
      {
        "domain_suffix": [
          "loli.io",
          "cjjd15.com",
          "cjjd18.com",
          "cjjd20.com",
          "dl.sourceforge.net",
          "asmr-100.com",
          "ghporxy.com",
          "dockerproxy.com",
          "winos.me",
          "kinh.cc",
          "dandanplay.net",
          "geilijiasu.com",
          "metacubex.one",
          "dl.google.com",
          "yacd.haishan.me"
        ],
        "outbound": "bypass"
      },
      {
        "geosite": "googlefcm",
        "outbound": "googlefcm"
      },
      {
        "geosite": [
          "apple-cn",
          "google-cn"
        ],
        "outbound": "bypass"
      },
      {
        "geosite": "category-games@cn",
        "outbound": "bypass"
      },
      {
        "geosite": "geolocation-!cn",
        "outbound": "proxy"
      },
      {
        "geosite": "cn",
        "geoip": "cn",
        "outbound": "bypass"
      },
      {
        "geosite": "private",
        "geoip": "private",
        "outbound": "direct"
      }
    ],
    "geoip": {
      "path": "geoip.db",
      "download_url": "https://ghproxy.com/https://github.com/soffchen/sing-geoip/releases/latest/download/geoip.db",
      "download_detour": "direct"
    },
    "geosite": {
      "path": "geosite.db",
      "download_url": "https://ghproxy.com/https://github.com/soffchen/sing-geosite/releases/latest/download/geosite.db",
      "download_detour": "direct"
    }
  },
  "dns": {
    "final": "proxy",
    "rules": [
      {
        "geosite": "category-ads-all",
        "disable_cache": true,
        "server": "block"
      },
      // {
      //   "domain_suffix": [
      //     "cjjd15.com",
      //     "cjjd18.com",
      //     "cjjd20.com",
      //     "dl.sourceforge.net",
      //     "asmr-100.com",
      //     "ghporxy.com",
      //     "dockerproxy.com",
      //     "winos.me",
      //     "kinh.cc",
      //     "dandanplay.net",
      //     "geilijiasu.com",
      //     "metacubex.one",
      //     "dl.google.com",
      //     "yacd.haishan.me"
      //   ],
      //   "server": "bypass"
      // },
      {
        "geosite": "category-games@cn",
        "server": "bypass"
      },
      {
        "geosite": "geolocation-!cn",
        "server": "proxy"
      },
      {
        "geosite": "cn",
        "server": "bypass"
      },
      {
        "geosite": "private",
        "server": "direct"
      }
    ],
    "servers": [
      {
        "tag": "proxy",
        "address": "udp://127.0.0.1:9921",
        // "strategy": "prefer_ipv6",
        "detour": "direct"
      },
      {
        "tag": "bypass",
        "address": "tls://223.5.5.5",
        // "strategy": "prefer_ipv6"
        "detour": "bypass"
      },
      {
        "tag": "direct",
        "address": "dhcp://auto",
        "detour": "direct"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ]
  },
  "outbounds": [
    {
      "tag": "fallback",
      "outbounds": [
        "geph_1",
        "geph_2",
        "geph_3",
        "geph_4",
        "geph_default",
        "bypass"
      ],
      "url": "http://www.apple.com/library/test/success.html",
      "interval": "30s",
      "tolerance": 9999,
      "type": "urltest"
    },
    {
      "tag": "proxy",
      "outbounds": [
        "fallback",
        "geph_default",
        "geph_1",
        "geph_2",
        "geph_3",
        "geph_4"
      ],
      "type": "selector"
    },
    {
      "tag": "googlefcm",
      "outbounds": [
        "bypass",
        "proxy"
      ],
      "type": "selector"
    },
    // {
    //   "tag": "game",
    //   "outbounds": [
    //     "bypass",
    //     "proxy"
    //   ],
    //   "type": "selector"
    // },
    // {
    //   "tag": "game-udp",
    //   "outbounds": [
    //     "bypass",
    //     "proxy"
    //   ],
    //   "type": "selector"
    // },
    // {
    //   "tag": "bilibili",
    //   "outbounds": [
    //     "bypass",
    //     "proxy"
    //   ],
    //   "type": "selector"
    // },
    // {
    {
      "tag": "geph_default",
      "server": "127.0.0.1",
      "server_port": 9910,
      "type": "http"
    },
    {
      "tag": "geph_1",
      "server": "127.0.0.1",
      "server_port": 9901,
      // "domain_strategy": "ipv4_only",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "geph_2",
      "server": "127.0.0.1",
      "server_port": 9912,
      "type": "http"
    },
    {
      "tag": "geph_3",
      "server": "127.0.0.1",
      "server_port": 9913,
      "type": "http"
    },
    {
      "tag": "geph_4",
      "server": "127.0.0.1",
      "server_port": 9914,
      "type": "http"
    },
    {
      "tag": "direct",
      "type": "direct"
    },
    {
      "tag": "bypass",
      // "domain_strategy": "prefer_ipv6",
      "type": "direct"
    },
    {
      "tag": "block",
      "type": "block"
    },
    {
      "tag": "dns-out",
      "type": "dns"
    }
  ],
  "inbounds": [
    {
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 7890,
      "set_system_proxy": false,
      "sniff": true,
      "sniff_override_destination": false,
      // "domain_strategy": "prefer_ipv4",
      "type": "mixed"
    }
  ],
  "log": {
    "level": "debug",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "yacd",
      "secret": "touhoured",
      "store_selected": true
    }
  }
}