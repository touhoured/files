// first launch need download rules-set.
// or change dns.final to "direct"
{
  "route": {
    // "final": "proxy",
    // "auto_detect_interface": true,
    // "default_network_strategy": "fallback",
    // "default_fallback_delay": "300ms",
    // "default_interface": "WLAN",
    "rules": [
      {
        // "inbound": "in-tun",
        // "timeout": "1s",
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "network": "udp",
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "rule_set": "rule-geph",
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "rule_set": "geosite-private",
        "outbound": "direct"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "proxy"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "protocol": "stun"
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "quic"
          }
        ],
        "action": "reject"
      },
      {
        "rule_set": [
          "rule-anti-ip-attribution-reject",
          "rule-MeepoMaster",
          "rule-Cats-Team",
          // "rule-ppfeufer",
          // "rule-AdGuardSDNSFilter",
          "geosite-category-httpdns-cn",
          "geosite-category-ads-all",
          "rule-custom-block"
        ],
        "action": "reject"
      },
      {
        "rule_set": "rule-custom-direct",
        "outbound": "direct"
      },
      {
        "rule_set": "rule-anti-ip-attribution-direct",
        "outbound": "direct"
      },
      {
        "rule_set": "rule-anti-ip-attribution-proxy",
        "outbound": "anti-ip-attribution"
      },
      {
        "domain_suffix": [
          "extshort.weixin.qq.com",
          "short.weixin.qq.com"
        ],
        "outbound": "wechat"
      },
      {
        "rule_set": "geosite-googlefcm",
        "outbound": "googlefcm"
      },
      {
        "rule_set": "geosite-onedrive",
        "outbound": "onedrive"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "outbound": "direct"
      },
      // {
      //   "rule_set": "geosite-geolocation-!cn",
      //   "outbound": "proxy"
      // },
      {
        "action": "resolve"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geoip-cn"
          },
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          }
        ],
        "outbound": "direct"
      },
      {
        "rule_set": [
          // "geosite-tld-cn",
          // "geoip-cn",
          "geosite-cn"
        ],
        "outbound": "direct"
      },
      {
        "outbound": "final"
      }
    ],
    "rule_set": [
      {
        "tag": "rule-custom-block",
        "rules": [
          {
            "domain": [
              "block.test", // only test
              "black.qimo.ink", // biliroming blacklist
              "2fa.qwq2333.top", // QAuxiliary
              "api.qwq2333.top", // QAuxiliary
              "ntp.msn.cn" // edge startpage
            ],
            "domain_suffix": [
              ".mcdn.bilivideo.cn",
              ".szbdyd.com"
            ],
            "domain_regex": [
              "cn-.+\\.bilivideo\\.com$",
              "-pcdn-.?\\.biliapi\\.net$"
            ]
          }
        ],
        "type": "inline"
      },
      {
        "tag": "rule-custom-direct",
        "rules": [
          {
            "domain_suffix": [
              "bsbsb.top",
              "ghfast.top",
              "ghproxy.cc",
              "deeplx.org",
              "dandanplay.net",
              "metacubex.one",
              "yacd.haishan.me"
            ]
          }
        ],
        "type": "inline"
      },
      {
        "tag": "rule-geph",
        "rules": [
          {
            "domain_suffix": [
              "cdn77.org",
              "cdn77.com"
            ]
          },
          {
            "process_name": [
              "geph5-client-gui.exe",
              "geph4-client.exe",
              "geph5-client.exe",
              "geph5-client",
              "geph4-client"
            ]
          }
        ],
        "type": "inline"
      },
