      },
      {
        "rule_set": [
          "geosite-geolocation-cn",
          "geosite-tld-cn",
          "geosite-cn"
        ],
        "server": "direct"
      },
      // {
      //   "rule_set": "geosite-geolocation-!cn",
      //   "server": "proxy"
      // },
