{
  "route": {
    "final": "out-proxy", // default proxy, whilelist mode
    // "auto_detect_interface": true,       // disable to fix multi interfaces
    // "default_interface": "WLAN",         // disable to fix multi interfaces
    "rules": [
      { // bypass core
        "process_name": [
          "geph4-client.exe",
          "geph4-client"
        ],
        "outbound": "out-direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "out-proxy"
      },
      {
        "clash_mode": "Direct",
        "outbound": "out-direct"
      },
      { // dns
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "inbound": "in-dns"
          },
          {
            "port": 53
          }
        ],
        "outbound": "out-dns"
      },
      { // bypass lan, new priority, may affect match
        "ip_is_private": true,
        "rule_set": "geosite-private",
        "outbound": "out-direct"
      },
      { // ad
        "rule_set": [
          "BlockHttpDNS",
          "geosite-category-ads-all"
        ],
        "domain_suffix": [
          ".mcdn.bilivideo.cn",
          ".szbdyd.com"
        ],
        "domain_regex": [
          "cn-[a-zA-Z0-9-]+\\.bilivideo\\.com",
          "[a-zA-Z0-9-]+-pcdn-[a-zA-Z0-9-]+\\.biliapi\\.net"
        ],
        "outbound": "out-block"
      },
      { // custom rule
        "rule_set": "custom-rule",
        "outbound": "out-direct"
      },
      { // custom rule
        "domain": [
          "dispatchosglobal.yuanshen.com",
          "overseauspider.yuanshen.com"
        ],
        "outbound": "out-proxy"
      },
      {
        "rule_set": "geosite-category-games@cn",
        "outbound": "out-direct"
      },
      {
        "rule_set": "geosite-googlefcm",
        "outbound": "out-googlefcm"
      },
      // { // china cdn for foreign company
      //   "rule_set": [
      //     "geosite-apple@cn",
      //     "geosite-google@cn"
      //   ],
      //   "outbound": "out-direct"
      // },
      { // not china
        "rule_set": "geosite-geolocation-!cn",
        "outbound": "out-proxy"
      },
      { // china
        "rule_set": [
          "geosite-geolocation-cn",
          "geosite-tld-cn",
          "geoip-cn"
        ],
        "outbound": "out-direct"
      }
      // {                                   // bypass lan, old priority, may affect performance
      //   "ip_is_private": true,
      //   "rule_set": "geosite-private",
      //   "outbound": "out-direct"
      // }
    ],
    "rule_set": [
      {
        "tag": "BlockHttpDNS",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/bm7/BlockHttpDNS.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-ads-all.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-category-games@cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-games@cn.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-googlefcm",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-googlefcm.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-apple@cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-apple@cn.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-google@cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-google@cn.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-geolocation-!cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-!cn.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-geolocation-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-cn.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-tld-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-tld-cn.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geoip-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "geosite-private",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-private.srs",
        "download_detour": "out-proxy",
        "type": "remote"
      },
      {
        "tag": "custom-rule",
        "format": "source",
        "path": "custom-rule.json",
        "type": "local"
      }
    ]
  },
  "dns": {
    // "final": "dns-proxy",
    "rules": [
      // {
      //   "outbound": "any",
      //   "server": "local"
      // },
      // {
      //   "clash_mode": "Global",
      //   "server": "google"
      // },
      // {
      //   "clash_mode": "Direct",
      //   "server": "google"
      // },
      {
        "rule_set": [
          "BlockHttpDNS",
          "geosite-category-ads-all"
        ],
        "domain_suffix": [
          ".mcdn.bilivideo.cn",
          ".szbdyd.com"
        ],
        "domain_regex": [
          "cn-[a-zA-Z0-9-]+\\.bilivideo\\.com",
          "[a-zA-Z0-9-]+-pcdn-[a-zA-Z0-9-]+\\.biliapi\\.net"
        ],
        "disable_cache": true,
        "server": "dns-success"
      },
      {
        "rule_set": [
          "geosite-apple@cn",
          "geosite-google@cn"
        ],
        "server": "out-direct"
      },
      {
        "rule_set": "geosite-category-games@cn",
        "server": "dns-direct"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "dns-proxy"
      },
      {
        "rule_set": [
          "geosite-tld-cn",
          "geosite-geolocation-cn"
        ],
        "server": "dns-direct"
      }
    ],
    "servers": [
      //  h3://1.1.1.1/dns-query
      //  https://1.1.1.1/dns-query
      //  tcp://1.1.1.1
      //  127.0.0.1:15353
      // quic://dns.adguard-dns.com
      {
        "tag": "dns-proxy",
        "address": "tcp://1.0.0.1",
        // "strategy": "ipv4_only",
        "address_resolver": "dns-local",
        "detour": "out-proxy"
      },
      {
        "tag": "dns-direct",
        "address": "tls://223.5.5.5",
        "detour": "out-direct"
      },
      {
        "tag": "dns-local",
        "address": "local",
        "detour": "out-direct"
      },
      {
        "tag": "dns-success",
        "address": "rcode://success"
      },
      {
        "tag": "dns-block",
        "address": "rcode://refused"
      }
    ]
  },
  "outbounds": [
    {
      "tag": "out-fallback",
      "outbounds": [
        "out-geph-default",
        "out-geph-1",
        "out-geph-2",
        "out-geph-3",
        "out-geph-4"
      ],
      "url": "http://www.apple.com/library/test/success.html",
      "interval": "30s",
      "tolerance": 9999,
      "type": "urltest"
    },
    {
      "tag": "out-proxy",
      "outbounds": [
        "out-fallback",
        "out-geph-default",
        "out-geph-1",
        "out-geph-2",
        "out-geph-3",
        "out-geph-4"
      ],
      "type": "selector"
    },
    {
      "tag": "out-googlefcm",
      "outbounds": [
        "out-direct",
        "out-proxy"
      ],
      "type": "selector"
    },
    // {
    //   "tag": "out-game",
    //   "outbounds": [
    //     "out-direct",
    //     "out-proxy"
    //   ],
    //   "type": "selector"
    // },
    // {
    //   "tag": "out-game-udp",
    //   "outbounds": [
    //     "out-direct",
    //     "out-proxy"
    //   ],
    //   "type": "selector"
    // },
    // {
    //   "tag": "out-bilibili",
    //   "outbounds": [
    //     "out-direct",
    //     "out-proxy"
    //   ],
    //   "type": "selector"
    // },
    {
      "tag": "out-geph-default",
      "server": "127.0.0.1",
      "server_port": 9909,
      // "domain_strategy": "ipv4_only",
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-geph-1",
      "server": "127.0.0.1",
      "server_port": 9908,
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-geph-2",
      "server": "127.0.0.1",
      "server_port": 9907,
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-geph-3",
      "server": "127.0.0.1",
      "server_port": 9906,
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-geph-4",
      "server": "127.0.0.1",
      "server_port": 9905,
      "network": "tcp",
      "version": "5",
      "type": "socks"
    },
    {
      "tag": "out-direct",
      "type": "direct"
    },
    {
      "tag": "out-block",
      "type": "block"
    },
    {
      "tag": "out-dns",
      "type": "dns"
    }
  ],
  "inbounds": [
    // {
    //   "tag": "tun-in",
    //   "inet4_address": "172.19.0.1/30",
    //   "inet6_address": "fdfe:dcba:9876::1/126",
    //   "auto_route": true,
    //   "strict_route": false,
    //   "stack": "mixed",    //default
    //   "sniff": true,
    //   "sniff_override_destination": false,
    //   "type": "tun"
    // },
    {
      "tag": "in-dns",
      "listen": "::",
      "listen_port": 1053,
      "type": "direct"
    },
    {
      "tag": "in-mixed",
      "listen": "::",
      "listen_port": 7890,
      "set_system_proxy": false,
      "sniff": true,
      "sniff_override_destination": false,
      // "domain_strategy": "prefer_ipv4",
      "type": "mixed"
    }
  ],
  "log": {
    "level": "info",
    "timestamp": false
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "cache.db"
    },
    "clash_api": {
      "external_controller": ":9090",
      "secret": "touhoured",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/gh-pages.zip",
      "external_ui_download_detour": "out-proxy",
      "external_ui": "xd"
    }
  }
}